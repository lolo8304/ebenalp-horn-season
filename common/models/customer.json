{
  "name": "customer",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "category": {
      "type": "string",
      "required": true,
      "comments": "ex. Erwachsene, Jugendliche, Kinder, Hornli"
    },
    "dateOfBirth": {
      "type": "date",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "street": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "userId": {
      "type": "number",
      "id": false,
      "required": true
    },
    "id": {
      "type": "number",
      "id": true,
      "required": true,
      "index": true
    },
    "season": {
      "type": "string",
      "required": true,
      "index": false,
      "comments": "example 2015-2016"
    },
    "device_alias": {
      "type": "string",
      "index": true
    },
    "email": {
      "type": "string",
      "id": false,
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "trackings": {
      "type": "hasMany",
      "model": "tracking",
      "foreignKey": "customer_id"
    },
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
